{
	"name": "reagentbuilder",
	"version": "1.0.0",
	"type": "module",
	"description": "An enterprise-grade AI agent framework based on LangChain and LangGraph, featuring dynamic tools, interceptors, breakpoints, and performance monitoring.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"build": "tsc --project tsconfig.build.json",
		"build:types": "tsc --emitDeclarationOnly --project tsconfig.build.json",
		"dev": "tsc --project tsconfig.build.json --watch",
		"prepublishOnly": "pnpm run build",
		"clean": "rimraf dist"
	},
	"keywords": [
		"agent",
		"langchain",
		"ai",
		"llm",
		"intelligent-agent",
		"interceptor",
		"breakpoint",
		"tool-management"
	],
	"author": "reuAC",
	"license": "Apache-2.0",
	"dependencies": {
		"@langchain/anthropic": "^0.3.27",
		"@langchain/core": "^0.3.75",
		"@langchain/langgraph": "^0.4.9",
		"@langchain/openai": "^0.6.11",
		"zod": "^4.1.5"
	},
	"devDependencies": {
		"@types/node": "^18.0.0",
		"rimraf": "^5.0.0",
		"typescript": "^5.9.2"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/reuAC/reagentbuilder.git"
	},
	"homepage": "https://github.com/reuAC/reagentbuilder",
	"bugs": {
		"url": "https://github.com/reuAC/reagentbuilder/issues"
	}
}
